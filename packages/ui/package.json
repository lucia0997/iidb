{
  "name": "@df/ui",
  "version": "0.0.0",
  "description": "Data Factory React components library.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:types": "tsc -b ../../tsconfig.build.json",
    "build:code-cjs": "babel src --extensions \".ts,.tsx\" --out-dir dist/cjs --source-maps --config-file ../../babel.config.cjs.js",
    "build:code-esm": "babel src --extensions \".ts,.tsx\" --out-dir dist/esm --source-maps --config-file ../../babel.config.esm.js",
    "build": "npm run build:types && npm run build:code-cjs && npm run build:code-esm",
    "pack": "npm run build && npm pack --pack-destination ../../"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@airbus/components-react": "^3.1.0",
    "@airbus/icons": "^3.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.8.3"
  }
}
